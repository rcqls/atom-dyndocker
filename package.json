{
  "name": "dyndocker",
  "main": "./lib/main",
  "version": "0.3.3",
  "description": "dyndoc docker from atom",
  "activationCommands": {
    "atom-workspace": [
      "dyndocker:eval",
      "dyndocker:atom-dyndoc",
      "dyndocker:compile",
      "dyndocker:task-write-dyn2tex2pdf",
      "dyndocker:task-write-dyn2html",
      "dyndocker:toggle",
      "dyndocker:restart"
    ]
  },
  "repository": "https://github.com/rcqls/atom-dyndocker",
  "license": "LGPL",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.0",
    "net": ">1.0.0",
    "fs-plus": "2.x",
    "underscore-plus": "1.x",
    "coffee-script": "1.7.x"
  }
}
